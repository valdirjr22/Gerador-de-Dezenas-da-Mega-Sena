08 32 52 64 77`;

// Define the lottery configurations
const LOTTERY_CONFIGS = {
    megaSena: {
        name: 'Mega-Sena',
        numbersPerGame: 6,
        maxNumber: 60,
        description: 'A Mega-Sena exige a escolha de 6 números entre 1 e 60. Os jogos são gerados com base na frequência dos números nos sorteios anteriores.',
        rawData: MEGA_SENA_RAW_DRAWS,
    },
    quina: {
        name: 'Quina',
        numbersPerGame: 5,
        maxNumber: 80,
        description: 'A Quina exige a escolha de 5 números entre 1 e 80. Os jogos são gerados com base na frequência dos números nos sorteios anteriores.',
        rawData: QUINA_RAW_DRAWS,
    },
};

// Main App component for the Mega-Sena number generator
const App = () => {
    // State to store the selected lottery type (e.g., 'megaSena', 'quina')
    const [lotteryType, setLotteryType] = useState('megaSena');
    // State to store the number of games to generate
    const [numGames, setNumGames] = useState(1);
    // State to store the generated games
    const [generatedGames, setGeneratedGames] = useState([]);
    // State to store the message for the user (e.g., error messages)
    const [message, setMessage] = useState('');

    // Get the current lottery configuration based on the selected type
    const currentConfig = LOTTERY_CONFIGS[lotteryType];
    const { numbersPerGame, maxNumber, name, description, rawData } = currentConfig;

    // State to store the raw input of past draws, dynamically updated based on lotteryType
    // This state's value is directly bound to the textarea for display.
    const [pastDrawsInput, setPastDrawsInput] = useState(rawData);

    // Effect to update pastDrawsInput when lotteryType changes
    // This ensures the textarea content reflects the selected lottery's data.
    useEffect(() => {
        setPastDrawsInput(currentConfig.rawData);
        setGeneratedGames([]); // Clear generated games when lottery type changes
        setMessage(''); // Clear messages
    }, [lotteryType, currentConfig.rawData]); // Depend on lotteryType and its rawData to trigger update

    // Memoized function to parse the raw draw data into an array of arrays of numbers
    // This re-runs only when pastDrawsInput (the displayed raw data) or currentConfig changes.
    const parsedDraws = useMemo(() => {
        const draws = [];
        const lines = pastDrawsInput.split('\n'); // Use the dynamically updated pastDrawsInput for parsing
        let tempParseErrors = [];

        try {
            for (let i = 0; i < lines.length; i++) {
                const line = lines[i];
                // Check if the current line is a "Concurso" header
                const drawMatch = line.match(/Concurso:\s*\d+\s*-\s*(\d{2}\/\d{2}\/\d{4})\s*\(.+\)/);

                if (drawMatch) {
                    const numbersLineIndex = i + 1;
                    if (numbersLineIndex < lines.length) {
                        const numbersLine = lines[numbersLineIndex];
                        // Remove "Mais Detalhes" and any other non-numeric text, then split by spaces
                        const cleanedNumbersLine = numbersLine.replace(/[^0-9\s]/g, '').trim();
                        const numbers = cleanedNumbersLine.split(/\s+/).filter(Boolean).map(Number); // Convert to numbers immediately

                        // Validate against the *current lottery type's* configuration
                        // This ensures only relevant draws are parsed for frequency analysis and game generation.
                        if (numbers.length === currentConfig.numbersPerGame) {
                            const allNumbersValid = numbers.every(num => num >= 1 && num <= currentConfig.maxNumber);
                            if (allNumbersValid && new Set(numbers).size === numbers.length) {
                                 draws.push(numbers.sort((a, b) => a - b));
                            } else {
                                // Optionally, collect specific errors for invalid numbers within a valid draw structure
                                // tempParseErrors.push(`Erro na linha ${numbersLineIndex + 1} (Números): Esperado ${currentConfig.numbersPerGame} números únicos entre 1 e ${currentConfig.maxNumber}. Encontrado: ${cleanedNumbersLine}`);
                            }
                        }
                    }
                }
            }
        } catch (error) {
            console.error("Erro ao analisar os sorteios: ", error);
            setMessage("Ocorreu um erro ao processar os dados dos sorteios. Verifique o formato.");
            return []; // Return empty array on error
        }


        // Set messages based on the outcome of parsing.
        // If no valid draws are found for the *selected* lottery type, display a message.
        if (draws.length === 0) {
            setMessage(`Nenhum sorteio válido para ${name} encontrado no texto fornecido. Verifique o formato e o tipo de loteria selecionado.`);
        } else {
            setMessage(''); // Clear message if parsing is successful
        }
        return draws;
    }, [pastDrawsInput, currentConfig, name]); // Depend on pastDrawsInput (the displayed data) and currentConfig

    // Memoized function to calculate the frequency of each number
    // This re-runs only when parsedDraws (the filtered and validated draws) or maxNumber changes.
    const numberFrequencies = useMemo(() => {
        const frequencies = new Array(maxNumber + 1).fill(0); // Initialize array for all possible numbers up to maxNumber
        parsedDraws.forEach(draw => {
            draw.forEach(number => {
                // Ensure number is within the configured maxNumber for the current lottery type
                if (number >= 1 && number <= maxNumber) {
                    frequencies[number]++;
                }
            });
        });
        // Convert frequencies into an array of objects for easier sorting and display
        return frequencies.map((count, number) => ({ number, count }))
            .filter(item => item.number > 0) // Exclude the 0 index (as numbers start from 1)
            .sort((a, b) => b.count - a.count); // Sort by count in descending order
    }, [parsedDraws, maxNumber]); // Depend on parsedDraws for the data and maxNumber for range

    // Function to generate a single game based on number frequencies
    const generateGame = () => {
        const game = new Set(); // Use a Set to ensure unique numbers
        const pool = [];

        // Create a pool of numbers weighted by their frequency
        numberFrequencies.forEach(item => {
            for (let i = 0; i < item.count; i++) {
                pool.push(item.number);
            }
        });

        // If no past draws or frequencies for the selected lottery type, generate random numbers
        if (pool.length === 0 || numberFrequencies.length === 0) {
            // Fallback to pure random if no valid frequencies are available
            while (game.size < numbersPerGame) {
                game.add(Math.floor(Math.random() * maxNumber) + 1);
            }
        } else {
            // Otherwise, pick numbers from the weighted pool
            while (game.size < numbersPerGame) {
                const randomIndex = Math.floor(Math.random() * pool.length);
                const chosenNumber = pool[randomIndex];
                if (chosenNumber >= 1 && chosenNumber <= maxNumber) { // Double check range
                    game.add(chosenNumber);
                }
            }
        }
        // Convert Set to Array and sort for display
        return Array.from(game).sort((a, b) => a - b);
    };

    // Handler for generating multiple games
    const handleGenerateGames = () => {
        if (numGames <= 0) {
            setMessage('Por favor, insira um número válido de jogos para gerar.');
            setGeneratedGames([]);
            return;
        }
        if (parsedDraws.length === 0) {
            setMessage(`Não há dados de sorteios válidos para a ${name} para gerar jogos com as configurações atuais. Por favor, cole os resultados dos sorteios anteriores ou ajuste o tipo de loteria.`);
            setGeneratedGames([]);
            return;
        }
        setMessage(''); // Clear any previous messages
        const newGames = [];
        for (let i = 0; i < numGames; i++) {
            newGames.push(generateGame());
        }
        setGeneratedGames(newGames);
    };

    return (
        <div className="min-h-screen bg-gradient-to-br from-green-400 to-green-600 p-4 font-inter text-gray-800 flex items-center justify-center">
            <div className="bg-white p-6 md:p-8 rounded-2xl shadow-xl w-full max-w-4xl space-y-6">
                <h1 className="text-3xl md:text-4xl font-extrabold text-center text-green-700 mb-6">
                    Gerador de Números para {name}
                </h1>

                {/* How to Use Section */}
                <div className="bg-blue-50 p-5 rounded-xl shadow-lg border border-blue-200">
                    <h2 className="text-xl md:text-2xl font-bold text-blue-800 mb-4">
                        Como Usar:
                    </h2>
                    <ol className="list-decimal list-inside text-gray-700 space-y-2 text-base">
                        <li>
                            <span className="font-semibold text-blue-700">1. Escolha o Tipo de Loteria:</span> Selecione "Mega-Sena" ou "Quina" para adaptar as regras e o intervalo de números.
                        </li>
                        <li>
                            <span className="font-semibold text-blue-700">2. Sorteios Anteriores (Visualização):</span> O campo abaixo mostrará os sorteios históricos da loteria selecionada.
                        </li>
                        <li>
                            <span className="font-semibold text-blue-700">3. Análise de Frequência:</span> A tabela abaixo mostrará a frequência de cada número com base nos sorteios exibidos e no tipo de loteria selecionado.
                        </li>
                        <li>
                            <span className="font-semibold text-blue-700">4. Gerar Novos Jogos:</span> Defina quantos jogos deseja gerar e clique no botão. As dezenas serão sugeridas com base na análise de frequência.
                        </li>
                    </ol>
                </div>

                {/* Lottery Type Selection */}
                <div className="bg-blue-50 p-5 rounded-xl shadow-lg border border-blue-200">
                    <h2 className="text-xl md:text-2xl font-bold text-blue-800 mb-4">
                        1. Escolha o Tipo de Loteria:
                    </h2>
                    <div className="flex flex-wrap gap-4 justify-center">
                        {Object.keys(LOTTERY_CONFIGS).map((key) => (
                            <label key={key} className="inline-flex items-center cursor-pointer">
                                <input
                                    type="radio"
                                    className="form-radio h-5 w-5 text-green-600"
                                    name="lotteryType"
                                    value={key}
                                    checked={lotteryType === key}
                                    onChange={(e) => setLotteryType(e.target.value)}
                                />
                                <span className="ml-2 text-lg font-semibold text-gray-700">
                                    {LOTTERY_CONFIGS[key].name}
                                </span>
                            </label>
                        ))}
                    </div>
                    <p className="text-center text-gray-600 mt-3 text-md">
                        {description}
                    </p>
                </div>

                {/* Past Draws Input Section */}
                <div className="bg-yellow-50 p-5 rounded-xl shadow-lg border border-yellow-200">
                    <h2 className="text-xl md:text-2xl font-bold text-yellow-800 mb-4">
                        2. Sorteios Anteriores (Visualização para {name}):
                    </h2>
                    <textarea
                        className="w-full p-3 border border-yellow-300 rounded-lg focus:ring-2 focus:ring-yellow-400 focus:border-transparent transition duration-200 ease-in-out text-gray-700 resize-y min-h-[200px]"
                        value={pastDrawsInput}
                        readOnly // Made read-only
                        placeholder={`Os resultados de sorteios anteriores da ${name} aparecerão aqui...`}
                    />
                    <p className="text-sm text-gray-600 mt-2">
                        Este campo exibe os dados de sorteios para a loteria selecionada. Para atualizar os dados, edite o código-fonte.
                    </p>
                </div>

                {/* Message Display Section */}
                {message && (
                    <div className={`p-4 rounded-lg shadow-md ${message.startsWith('Alguns erros') || message.startsWith('Nenhum sorteio válido') ? 'bg-red-100 border border-red-400 text-red-700' : 'bg-blue-100 border border-blue-400 text-blue-700'} `}>
                        <p className="font-semibold">{message}</p>
                    </div>
                )}

                {/* Frequency Analysis Section */}
                <div className="bg-purple-50 p-5 rounded-xl shadow-lg border border-purple-200">
                    <h2 className="text-xl md:text-2xl font-bold text-purple-800 mb-4">
                        3. Análise de Frequência dos Números:
                    </h2>
                    <p className="text-gray-600 mb-4">
                        Esta tabela mostra a frequência de cada número com base nos sorteios anteriores válidos para a **{name}** (que possui {numbersPerGame} dezenas, no intervalo de 1 a {maxNumber}). Os números são ordenados do mais frequente para o menos frequente.
                    </p>
                    <div className="max-h-60 overflow-y-auto pr-2">
                        <table className="min-w-full bg-white rounded-lg shadow-md overflow-hidden">
                            <thead className="bg-purple-200 sticky top-0">
                                <tr>
                                    <th className="py-2 px-4 text-left text-sm font-semibold text-purple-700">Número</th>
                                    <th className="py-2 px-4 text-left text-sm font-semibold text-purple-700">Frequência</th>
                                </tr>
                            </thead>
                            <tbody>
                                {numberFrequencies.length > 0 ? (
                                    numberFrequencies.map((item) => (
                                        <tr key={item.number} className="border-b border-purple-100 last:border-b-0 hover:bg-purple-50">
                                            <td className="py-2 px-4 text-gray-700">{String(item.number).padStart(2, '0')}</td>
                                            <td className="py-2 px-4 text-gray-700">{item.count}</td>
                                        </tr>
                                    ))
                                ) : (
                                    <tr>
                                        <td colSpan="2" className="py-4 text-center text-gray-500">
                                            Nenhum dado de sorteio válido encontrado para analisar a frequência com as configurações atuais.
                                        </td>
                                    </tr>
                                )}
                            </tbody>
                        </table>
                    </div>
                </div>

                {/* Generate Games Section */}
                <div className="bg-green-50 p-5 rounded-xl shadow-lg border border-green-200">
                    <h2 className="text-xl md:text-2xl font-bold text-green-800 mb-4">
                        4. Gerar Novos Jogos de {name}:
                    </h2>
                    <div className="flex flex-col sm:flex-row items-center gap-4 mb-4">
                        <label htmlFor="numGames" className="text-lg font-semibold text-gray-700">
                            Número de Jogos a Gerar:
                        </label>
                        <input
                            type="number"
                            id="numGames"
                            className="w-24 p-2 border border-green-300 rounded-lg focus:ring-2 focus:ring-green-400 focus:border-transparent transition duration-200 ease-in-out text-center text-gray-700"
                            value={numGames}
                            onChange={(e) => setNumGames(Number(e.target.value))}
                            min="1"
                        />
                    </div>
                    <button
                        onClick={handleGenerateGames}
                        className="w-full bg-green-600 hover:bg-green-700 text-white font-bold py-3 px-6 rounded-lg shadow-md transition duration-300 ease-in-out transform hover:scale-105 focus:outline-none focus:ring-2 focus:ring-green-500 focus:ring-opacity-75"
                    >
                        Gerar Dezenas
                    </button>
                </div>

                {/* Generated Games Section */}
                {generatedGames.length > 0 && (
                    <div className="bg-green-50 p-5 rounded-xl shadow-lg border border-green-200">
                        <h2 className="text-xl md:text-2xl font-bold text-green-800 mb-4">
                            5. Seus Jogos Gerados:
                        </h2>
                        <div className="grid grid-cols-1 gap-4">
                            {generatedGames.map((game, index) => (
                                <div key={index} className="bg-white p-4 rounded-lg shadow-md border border-green-100 flex flex-wrap items-center justify-center gap-3">
                                    <span className="text-gray-700 font-semibold mr-2">Jogo {index + 1}:</span>
                                    {game.map((num, i) => (
                                        <span key={i} className="bg-green-500 text-white font-bold text-lg w-10 h-10 flex items-center justify-center rounded-full shadow-md">
                                            {String(num).padStart(2, '0')}
                                        </span>
                                    ))}
                                </div>
                            ))}
                        </div>
                    </div>
                )}
            </div>
        </div>
    );
};

export default App;
